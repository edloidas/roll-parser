{
  "tags": {
    "allowUnknownTags": false
  },
    "source": {
    "include": "./src",
    "includePattern": ".js$"
  },
  "plugins": [ "plugins/markdown" ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "api/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "default": {
      "layoutFile": "node_modules/docdash/tmpl/layout.tmpl",
      "includeDate": false
    }
  },
  "docdash": {
    "sort": false
  }
}
