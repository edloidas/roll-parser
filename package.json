{
  "name": "roll-parser",
  "version": "2.3.2",
  "description": "Parser for classic (2d6+1), simple (2 6 1), and WoD (4d10!>6f1) dice rolls.",
  "main": "index.js",
  "bin": "bin/roll-parser.js",
  "keywords": [
    "parser",
    "dice",
    "roll",
    "rpg",
    "dnd",
    "wod",
    "random",
    "d20"
  ],
  "homepage": "https://github.com/edloidas/roll-parser#readme",
  "bugs": {
    "url": "https://github.com/edloidas/roll-parser/issues"
  },
  "license": "MIT",
  "author": "Mikita Taukachou <edloidas@gmail.com> (https://edloidas.com)",
  "files": [
    "index.js",
    "bin",
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edloidas/roll-parser.git"
  },
  "scripts": {
    "test": "jest",
    "cover": "jest --coverage",
    "report": "node_modules/.bin/jest --coverage && cat coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "fix": "eslint --fix .",
    "api": "rimraf api/ && typedoc --out api/ src/",
    "clean": "rimraf dist/* index.js",
    "build:node": "esbuild --bundle --platform=node --outfile=index.js index.ts",
    "build:browser": "esbuild --bundle --target=es2020 --global-name=rollParser --sourcemap=inline --outfile=dist/roll-parser.js index.ts",
    "build:browser:min": "esbuild --bundle --target=es2020 --global-name=rollParser --minify-syntax --minify-whitespace --outfile=dist/roll-parser.min.js index.ts",
    "build": "npm run build:node && npm run build:browser && npm run build:browser:min",
    "prepush": "npm test",
    "prepublishOnly": "npm run fix && npm test && npm run clean && npm run build"
  },
  "dependencies": {
    "minimist": "^1.2.5",
    "typedoc": "^0.19.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/minimist": "^1.2.0",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "coveralls": "^3.1.0",
    "esbuild": "^0.7.14",
    "eslint": "^7.11.0",
    "husky": "^4.3.0",
    "jest": "^26.5.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">= 14.13.1",
    "npm": ">= 6.13.7"
  }
}
